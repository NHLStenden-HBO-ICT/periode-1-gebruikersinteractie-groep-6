<UserControl
	x:Class="periode_1_gebruikersinteractie_groep_6.Windows.Game.PlayerView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:periode_1_gebruikersinteractie_groep_6.Windows.Game"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="1080"
	d:DesignWidth="960"
	mc:Ignorable="d">
	<Canvas x:Name="canvas">
		<Image
			x:Name="Road1"
			Canvas.Top="-1080"
			Width="960"
			Height="1080"
			HorizontalAlignment="Left" />
		<Image
			x:Name="Road2"
			Width="960"
			Height="1080"
			HorizontalAlignment="Left" />
		<Image
			x:Name="Car"
			Width="200"
			Height="200" />
		<Viewbox
			Width="200"
			Height="200"
			Stretch="Uniform" />
		<TextBlock
			x:Name="debug"
			Width="960"
			Height="1080"
			FontSize="96" />
	</Canvas>
</UserControl>
