<UserControl
	x:Class="periode_1_gebruikersinteractie_groep_6.Windows.RaceWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:periode_1_gebruikersinteractie_groep_6.Windows"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="{x:Static SystemParameters.PrimaryScreenHeight}"
	d:DesignWidth="{x:Static SystemParameters.PrimaryScreenWidth}"
	mc:Ignorable="d">

    <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>


            <Image
                Grid.Column="0"
			Width="{x:Static SystemParameters.FullPrimaryScreenWidth}"
			Height="{x:Static SystemParameters.PrimaryScreenHeight}"
			Source="../Assets/LeftSideRacingCircuit.png"/>
            <Image Grid.Column="1"
			Width="{x:Static SystemParameters.FullPrimaryScreenWidth}"
			Height="{x:Static SystemParameters.PrimaryScreenHeight}"
			Source="../Assets/RightSideRacingCircuit.png"/>



        </Grid>
   
</UserControl>
